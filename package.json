{
  "name": "imas-idollist-cooccurence",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm preprocess && tsc -b && vite build --mode client && vite build --mode ssg && node scripts/fix-base-path.ts",
    "preview": "vite preview",
    "scrape": "node scripts/scrape.ts",
    "scrape:list": "node src/scraper/index.ts list",
    "scrape:details": "node src/scraper/index.ts details",
    "preprocess": "node src/transformer/index.ts",
    "push:data": "node scripts/push-data-branch.ts",
    "update:data": "pnpm scrape && pnpm push:data",
    "download:images": "node scripts/downloadImages.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint src app tests",
    "lint:fix": "oxlint --fix src app tests",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "knip",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/react-renderer": "1.0.1",
    "hono": "4.11.1",
    "honox": "0.1.52",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.0"
  },
  "devDependencies": {
    "@hono/vite-ssg": "0.3.0",
    "@types/jsdom": "27.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "jsdom": "^25.0.1",
    "knip": "^5.41.1",
    "oxlint": "^0.15.4",
    "playwright": "^1.49.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "playwright"
    ],
    "packageManager": {
      "minimalReleaseAge": "10080"
    }
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "pnpm@9.15.1"
}
